{
   "name": "randTab",
   "version": "0.1",
   "manifest_version": 2,
   "description": "Switch to a random tab of the active browser window.",

   "background": { "page": "log.html" }, 

   "browser_action": {
      "default_icon": "dice.png",
      "default_title": "Random Tab"
   },

   "icons": {
      "48": "dice.png",
      "96": "dice.png",
      "128": "dice.png"
   },

   "permissions": [ 
      "tabs", 
      "windows",
      "https://*/*",
      "http://*/*"
   ]
}
